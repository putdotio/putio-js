{
  "name": "@putdotio/api-client",
  "version": "8.14.0",
  "description": "JS client for Put.io API",
  "keywords": [
    "putio",
    "put.io",
    "API client"
  ],
  "main": "dist/index.js",
  "module": "dist/api-client.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "tsc": "tsc --noEmit",
    "prepare": "tsdx build",
    "coveralls": "tsdx test --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "author": "Put.io <devs@put.io>",
  "license": "MIT",
  "homepage": "https://github.com/putdotio/putio.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/putdotio/putio.js.git"
  },
  "bugs": {
    "url": "https://github.com/putdotio/putio.js/issues"
  },
  "prettier": "@putdotio/prettier-config",
  "dependencies": {
    "axios": "^0.19.2",
    "event-emitter": "^0.3.5",
    "form-data": "^3.0.0",
    "js-base64": "^2.6.2",
    "qs": "^6.9.4",
    "urijs": "^1.19.5"
  },
  "devDependencies": {
    "@putdotio/prettier-config": "^1.0.0",
    "@types/event-emitter": "^0.3.3",
    "@types/js-base64": "^2.3.2",
    "@types/qs": "^6.9.5",
    "@types/urijs": "^1.19.13",
    "coveralls": "^3.1.0",
    "husky": "^4.2.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
